{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/users/register",
      "dest": "./index.js"
    },
    {
      "src": "/users/login",
      "dest": "./index.js"
    },
    {
      "src": "/users",
      "dest": "./index.js"
    },
    {
      "src": "/users/avatar",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "./index.js"
    }
  ]
}
